<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.flow.ProUserAssign"
        table="pro_user_assign"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="assignId"
            column="assignId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="deployId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="deployId"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="activityName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="activityName"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="roleId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="roleId"
            length="128"
            not-null="false"
            unique="false"
        />
         <property
            name="roleName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="roleName"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="username"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="username"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="isSigned"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isSigned"
            length="6"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
