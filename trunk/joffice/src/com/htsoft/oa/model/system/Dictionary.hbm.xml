<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.system.Dictionary"
        table="dictionary"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="dicId"
            column="dicId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="itemName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="itemName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="itemValue"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="itemValue"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="descp"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="descp"
            length="256"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
