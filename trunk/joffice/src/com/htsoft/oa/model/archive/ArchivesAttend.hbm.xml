<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.archive.ArchivesAttend"
        table="archives_attend"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="attendId"
            column="attendId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="userID"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userID"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="fullname"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="fullname"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="attendType"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="attendType"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="executeTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="executeTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="memo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="memo"
            length="1024"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Archives -->
		<many-to-one
			name="archives"
			class="com.htsoft.oa.model.archive.Archives"
            not-null="false"
			fetch="select"
		>	
			<column name="archivesId" />
		</many-to-one>
    </class>
</hibernate-mapping>
