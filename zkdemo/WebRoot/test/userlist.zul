<?page title="" contentType="text/html;charset=UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">
	<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" arg0="userList" ?>
	
	<window id="userList" border="normal" use="test.UserList">	
	<listbox id="listbox" model="@{userList.list}" mold="paging" paginal="${pg}" checkmark="true" multiple="true">
		<listhead>
			<listheader label="id" />
			<listheader label="姓名" />
		</listhead>
		<listitem self="@{each=user}" value="@{user.id}">
			<listcell label="@{user.id}"/>
			<listcell label="@{user.username}"/>
		</listitem>		
		</listbox>
	<paging id="pg"/>	
	
	</window>
</zk>