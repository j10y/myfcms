<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.archive.ArchRecType"
        table="arch_rec_type"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="recTypeId"
            column="typeId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="typeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="typeName"
            length="128"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to Department -->
		<many-to-one
			name="department"
			class="com.htsoft.oa.model.system.Department"
            not-null="false"
			fetch="select"
		>	
			<column name="depId" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to ArchivesDep -->
	<set name="archivesDeps"
	     table="archives_dep"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="typeId"/>
		</key>
		<one-to-many
			class="com.htsoft.oa.model.archive.ArchivesDep"
		/>
	</set>
	 <set name="archives"
	     table="arch_rec_type"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="typeId"/>
		</key>
		<one-to-many
			class="com.htsoft.oa.model.archive.Archives"
		/>
	</set>    
    </class>
</hibernate-mapping>
