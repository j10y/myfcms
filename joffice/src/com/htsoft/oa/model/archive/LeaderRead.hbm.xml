<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.archive.LeaderRead"
        table="leader_read"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="readId"
            column="readId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="leaderName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leaderName"
            length="64"
            not-null="true"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="leaderOpinion"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="leaderOpinion"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="createtime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createtime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="depLevel"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="depLevel"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="depName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="depName"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="isPass"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isPass"
            length="5"
            not-null="true"
            unique="false"
        />
         <property
            name="checkName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="checkName"
            length="128"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ArchivesDep
		<many-to-one
			name="archivesDep"
			class="com.htsoft.oa.model.archive.ArchivesDep"
            not-null="false"
			fetch="select"
		>	
			<column name="depArchId" />
		</many-to-one>
		 -->
    <!-- imported bi-directional many-to-one association to Archives -->
		<many-to-one
			name="archives"
			class="com.htsoft.oa.model.archive.Archives"
            not-null="false"
			fetch="select"
		>	
			<column name="archivesId" />
		</many-to-one>
    </class>
</hibernate-mapping>
