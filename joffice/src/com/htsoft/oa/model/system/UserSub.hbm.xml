<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.system.UserSub"
        table="user_sub"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="subId"
            column="subId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
          </id>
           <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="true"
            unique="false"
        />
       <!-- <many-to-one
			name="appUser"
			class="com.htsoft.oa.model.system.AppUser"
            not-null="false"
            lazy="false"
			fetch="select"
		>	
		 	<column name="userId" />
		</many-to-one>-->
     <!-- imported bi-directional many-to-one association to AppUser -->
		<many-to-one
			name="subAppUser"
			class="com.htsoft.oa.model.system.AppUser"
            not-null="false"
            column="subUserId"
            lazy="false"
			fetch="select"
		>	
		<!-- 	<column name="subUserId" /> -->
		</many-to-one>
    </class>
</hibernate-mapping>
