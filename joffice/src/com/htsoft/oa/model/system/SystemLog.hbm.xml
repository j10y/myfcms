<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.htsoft.oa.model.system.SystemLog"
        table="system_log"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="logId"
            column="logId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="username"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="username"
            length="128"
            not-null="true"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="createtime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createtime"
            length="19"
            not-null="true"
            unique="false"
        />
        <property
            name="exeOperation"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="exeOperation"
            length="512"
            not-null="true"
            unique="false"
        />
     </class>
</hibernate-mapping>
